{
    "builds": [
        { 
            "src": "/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ]
}
